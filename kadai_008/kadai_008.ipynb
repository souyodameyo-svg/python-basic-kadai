      "source": [
        "# 変数varに値を代入（例として15を代入）\n",
        "var = 35\n",
        "\n",
        "# 条件分岐\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "    # 3の倍数かつ5の倍数の場合\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "    # 3の倍数の場合\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    # 5の倍数の場合\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    # どの場合にも該当しない場合\n",
        "    print(var)"
      ]
    }
